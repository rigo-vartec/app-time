<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="/css/estilo-formulario-u.css">

<body class="body-light-grey">
    {{> navbar }}
    <div class="full-height">
        <div class="app-content-right">
            <div class="contenedor-img text-center">
                <img src="/logos/user-profile.png" alt="">
            </div>
            <div class="card-body mx-auto">
                <div class="card border-primary mb-3" style="max-width: 18rem;">
                    <div class="card-header"></div>
                    <div class="card-body text-primary">
                        <h5 class="card-title">Supervisor de calidad</h5>

                    </div>
                </div>
            </div>
        </div>
        <div class="app-content-left">
            {{> mensage }}
            <div class="card left-center text-center">
                <div class="row">
                    <div class="col">
                        <label for="">FECHA</label>
                        <p id="fecha" class="form-control"></p>
                    </div>
                    <div class="col">
                        <label for="">HORA</label>
                        <p id="hora" class="form-control"></p>
                    </div>
                </div>
            </div>
            <div class="card left-sub text-center" id="nav-tabContent">
                <div class="row mx-auto">
                    <div class="card text-white mb-3 text-center " style="width: 14rem;margin-right: 15px;">
                    <div class="card-header">AVERIAS
                    </div>
                    <div class="card-body">
                        <p class="card-text">Verifica el estatus de averias cerradas</p>
                        <div class="col">
                            <a href="/averias/operador/calidad/buscar" class="btn btn-success">BUSCAR</a>
                        </div>
                    </div>
                </div>
                </div>
            </div>
        </div>
    </div>
</body>

<script src="/js/reloj.js"> </script>   