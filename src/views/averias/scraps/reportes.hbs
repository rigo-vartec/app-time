  <div class="card-header">
    <H1>BUSCADOR DE REPORTES</H1>
  </div>
  <div class="card-body">
    <h5 class="card-title">Turno:Ma√±ana</h5>
    <form action="/links/" method="POST" id="test">
      <div class="form-row">
        <div class="col-sm-5">
          <label for="Select1">Fecha de busqueda</label>
          <input type="text" class="form-control" id="fecha-creacion" name="creation_date"
            placeholder="Fecha: dd/mm/yy">
        </div>
        <div class="col-sm-1">
          <label for="Select1">BUSCAR</label>
          <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
        </div>
        <div class="col-sm-1">
          <label for="Select1">REGRESAR</label>
          <a href="/links/inicio" class="btn btn-outline-info my-2 my-sm-0">Go-back</a>
        </div>
      </div>
    </form>
  </div>
</div>

